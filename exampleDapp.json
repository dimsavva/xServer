{
	"appname": "Wordpress",
	"ImageURL": "storage.storj.com/someimage.png",
	"deploymentVersion": "1.0.0",
	"jsonForms": {
		"schema": {
			"type": "object",
			"properties": {
				"users": {
					"type": "array",
					"items": {
						"type": "object",
						"title": "Users",
						"properties": {
							"firstname": {
								"type": "string"
							},
							"lastname": {
								"type": "string"
							},
							"email": {
								"type": "string",
								"format": "email"
							},
							"age": {
								"type": "number",
								"minimum": 0
							}
						},
						"required": [
							"firstname"
						]
					}
				}
			}
		},
		"uischema": {
			"type": "ListWithDetail",
			"scope": "#/properties/users",
			"options": {
				"detail": {
					"type": "VerticalLayout",
					"elements": [
						{
							"type": "HorizontalLayout",
							"elements": [
								{
									"type": "Control",
									"scope": "#/properties/firstname",
									"label": "First Name"
								},
								{
									"type": "Control",
									"scope": "#/properties/lastname",
									"label": "Last Name"
								}
							]
						},
						{
							"type": "Control",
							"scope": "#/properties/age",
							"label": "Age"
						},
						{
							"type": "Control",
							"scope": "#/properties/email",
							"label": "Email"
						}
					]
				}
			}
		},
		"defaultvalues": {
			"users": [
				{
					"firstname": "Max",
					"lastname": "Mustermann",
					"age": 25,
					"email": "max@mustermann.com"
				},
				{
					"firstname": "John",
					"lastname": "Doe",
					"age": 35,
					"email": "john@doe.com"
				}
			]
		}
	},
	"deploy_script": {
		"filename": "deploy_site.sh",
		"args": [
			{
				"name": "argument1",
				"value": "argument1value"
			},
			{
				"name": "argument2",
				"value": "argument2value"
			}
		],
		"content": ""
	},
	"files": [
		{
			"filename": "bin/acme.sh",
			"content": ""
		}
	]
}