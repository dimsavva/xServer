{
  "name": "x42-Server",
  "description": "x42 Server",
  "version": "4.0.0",
  "author": {
    "name": "x42 Protocol",
    "email": "contact@x42.info"
  },
  "license": "MIT",
  "homepage": "https://github.com/x42protocol/x42-Server",
  "keywords": [
    "x42",
    "dapp",
    "server"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "start": "npm run && npm-run-all -p ng:serve \"electron:serve -- {@}\" --",
    "mainnet": "npm start --",
    "testnet": "npm run && npm-run-all -p ng:serve \"electron:serve:testnet -- {@}\" --",
    "sidechain": "npm run && npm-run-all -p ng:serve \"electron:serve:sidechain -- {@}\" --",
    "sidechain:testnet": "npm run && npm-run-all -p ng:serve \"electron:serve:sidechain:testnet -- {@}\" --",
    "build": "npm run && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:serve:testnet": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve --testnet",
    "electron:serve:sidechain": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve --sidechain",
    "electron:serve:sidechain:testnet": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve --sidechain --testnet",
    "electron:local": "npm run build:prod && electron .",
    "package:linux": "npm run build:prod && electron-builder build --linux",
    "package:linuxarm": "npm run build:prod && electron-builder build --linux --armv7l",
    "package:win": "npm run build:prod && electron-builder build --windows",
    "package:win86": "npm run build:prod && electron-builder build --windows --ia32",
    "package:win64": "npm run build:prod && electron-builder build --windows --x64",
    "package:mac": "npm run build:prod && electron-builder build --mac",
    "package:sidechain:linux": "npm run build:prod && electron-builder build --linux --config ./electron-sidechain-builder.json",
    "package:sidechain:linuxarm": "npm run build:prod && electron-builder build --linux --armv7l --config ./electron-sidechain-builder.json",
    "package:sidechain:win": "npm run build:prod && electron-builder build --windows --config ./electron-sidechain-builder.json",
    "package:sidechain:win86": "npm run build:prod && electron-builder build --windows --ia32 --config ./electron-sidechain-builder.json",
    "package:sidechain:win64": "npm run build:prod && electron-builder build --windows --x64 --config ./electron-sidechain-builder.json",
    "package:sidechain:mac": "npm run build:prod && electron-builder build --mac --config ./electron-sidechain-builder.json",
    "test": "ng test",
    "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
    "lint": "ng lint",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "version": "node -p \"require('./package.json').version\""
  },
  "dependencies": {
    "@angular/cdk": "^7.3.7",
    "angular-font-awesome": "^3.1.2",
    "electron-context-menu": "0.14.0",
    "font-awesome": "^4.7.0",
    "ngx-pagination": "4.0.0",
    "ngx-qrcode2": "0.1.0",
    "primeflex": "^1.0.0",
    "primeicons": "^2.0.0",
    "primeng": "^8.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/animations": "^8.2.14",
    "@angular/cli": "^8.3.21",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.15",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.1.5",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.24",
    "bootstrap": "^4.4.1",
    "codelyzer": "^5.2.1",
    "core-js": "^3.0.1",
    "electron": "^7.1.9",
    "electron-builder": "^22.2.0",
    "electron-reload": "^1.5.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ngx-bootstrap": "^4.3.0",
    "ngx-clipboard": "^12.3.0",
    "ngx-electron": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^6.0.0",
    "rxjs": "^6.5.4",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "3.5.3",
    "wait-on": "^3.3.0",
    "webdriver-manager": "^13.0.0",
    "zone.js": "^0.10.2",
    "snyk": "^1.278.1"
  },
  "snyk": true
}
